= form_for(@skillset) do |f|
  - if @skillset.errors.any?
    #error_explanation
      %h2
        = pluralize(@skillset.errors.count, "error")
        prohibited this skillset from being saved:
      %ul
        - @skillset.errors.full_messages.each do |message|
          %li= message
  .row
    .col-lg-4.col-lg-offset-4
      .input-group
        .field
          = f.label :skills
          %br/
          = f.text_field :skills
        .actions
          = f.submit
